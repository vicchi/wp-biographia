msgid "Add"
msgstr "Dodaj"

#: /Users/gary/Projects/wp-biographia/wp-biographia.php:2539
msgid "Excluded Roles"
msgstr "wykluczone Role"

#: /Users/gary/Projects/wp-biographia/wp-biographia.php:2547
#: /Users/gary/Projects/wp-biographia/wp-biographia.php:2593
